{
  "short_name": "Hide Ads in search results",
  "name": "Hide Ads in search results",
  "description": "Hide Ads in search results in google.com page",
  "author": "Sima Amini",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
